number = {3-4},
pages = {309--320},
publisher = {Elsevier},
title = {{Reflection on-line or off-line: the role of learning technologies in encouraging students to reflect}},
url = {http://dx.doi.org/10.1016/S0360-1315(99)00052-4},
volume = {34},
